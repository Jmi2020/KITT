  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "kitty",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-id-KITT-2024"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-all",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all",
        "ha-sync-mode": "automatic"
      },
      "priority": 0
    },
    {
      "vhost": "/",
      "name": "dlx-policy",
      "pattern": "^(?!.*\\.dlx$).*",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "dlx",
        "dead-letter-routing-key": "dead-letter"
      },
      "priority": 1
    },
    {
      "vhost": "/",
      "name": "ttl-policy",
      "pattern": "^retry\\..*",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 300000,
        "expires": 3600000
      },
      "priority": 2
    }
  ],
  "queues": [
    {
      "name": "fabrication.events",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic",
        "x-max-length": 100000,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "research.tasks",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
